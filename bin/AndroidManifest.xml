<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bazaaut"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.bazaaut.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="DATABASE"
            android:value="sugar_example.db" />
        <meta-data
            android:name="VERSION"
            android:value="3" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.example" />

        <activity
            android:name="com.example.bazaaut.Baza"
            android:label="@string/title_activity_baza" >
        </activity>
        <activity
            android:name="com.example.bazaaut.DodajSamochod"
            android:label="@string/title_activity_dodaj_samochod" >
        </activity>
        <activity
            android:name="com.example.bazaaut.ListaSamochodow"
            android:label="@string/title_activity_lista_samochodow" >
        </activity>
        <activity
            android:name="com.example.bazaaut.ModelSamochodu"
            android:label="@string/title_activity_marka_samochodu" >
        </activity>
        <activity
            android:name="com.example.bazaaut.UsunSamochod"
            android:label="@string/title_activity_usun_samochod" >
        </activity>
    </application>

</manifest>